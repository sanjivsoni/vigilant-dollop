                        on_text: root.check3_otp()
					TextInput:
                        id: otp_4
						size_hint: 0.2,1
						font_size: '50sp'
						padding:15,5,15,0
						write_tab: False
                        on_text: root.check4_otp()
					TextInput:
                        id: otp_5
						size_hint: 0.2,1
						font_size: '50sp'
						padding:15,5,15,0
						write_tab: False
                        on_text: root.check5_otp()
					TextInput:
                        id: otp_6
						size_hint: 0.2,1
						font_size: '50sp'
						padding:15,5,15,0
						write_tab: False
                        on_text: root.check6_otp()
				BoxLayout:
					size_hint: 0.25,1

			BoxLayout:
				size_hint: 1,0.8
				orientation: 'horizontal'
